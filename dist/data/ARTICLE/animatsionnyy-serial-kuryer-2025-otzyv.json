        [
          [
            "strong",
            [
              [
                "Жанр: "
              ]
            ],
            {}
          ],
          [
            "сказка, приключения, нуар, комедия."
          ],
          [
            "br",
            [],
            {}
          ],
          [
            "strong",
            [
              [
                "Режиссер:"
              ]
