              [
                "Сахар — 2,5 стакана"
              ]
            ],
            {}
          ],
          [
            "li",
            [
              [
                "Ядра грецких орехов — полстакана"
              ]
            ],
            {}
          ],
          [
            "li",
            [
              [
                "Цукаты или твердый мармелад — 100 г"
              ]
            ],
            {}
          ],
          [
            "li",
            [
              [
                "Тертая сухая апельсиновая корка — 1 ч. л."
              ]
            ],
            {}
          ],
          [
            "li",
            [
              [
                "Сахарная пудра — полстакана"
              ]
            ],
            {}
